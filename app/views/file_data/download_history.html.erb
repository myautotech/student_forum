<div class="row">
  <div class="col-sm-12">
    <div class="box">
      <div class="box-header">
        <h3 class="box-title">Download history</h3>
      </div><!-- /.box-header -->
      <div class="box-body">
        <table class="table table-striped table-hover datatable">
          <thead>
            <tr>
              <th>Id</th>
              <th>File Name</th>
              <th>User</th>
              <th>Date</th>
            </tr>
          </thead>
          <tbody>
            <% @download_logs.each do |d| %>
              <tr>
                <td><%= d.id %></td>
                <td><%= d.file_datum.file_file_name %></td>
                <td><%= d.user.full_name %></td>
                <td><%= d.created_at.to_date %></td>
              </tr>
            <% end %>
          </tbody>
        </table>
      </div><!-- /.box-body -->
    </div><!-- /.box -->
  </div>
</div>
