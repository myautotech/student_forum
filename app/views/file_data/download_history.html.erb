<table id="datatable" class="table table-striped table-hover">
  <thead>
    <tr>
      <th>File Name</th>
      <th>User</th>
      <th>Date</th>
    </tr>
  </thead>
  <tbody>
    <% @download_logs.each do |d| %>
    <tr>
      <td><%= d.file_datum.file_file_name %></td>
      <td><%= d.user.full_name %></td>
      <td><%= d.created_at.to_date %></td>
    </tr>
    <% end %>
  </tbody>
</table>